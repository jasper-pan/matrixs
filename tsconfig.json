{
  "compilerOptions": {
    "sourceMap": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration":true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "strict": true,
    "lib": ["es6","dom"],
    "noImplicitAny": true,
    "jsx": "react",
    // "allowJs": true
  },
  "exclude": ["node_modules", "lib", "es"]
}
 